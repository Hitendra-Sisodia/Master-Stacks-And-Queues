//Function to push an element into stack using two queues. Method - 1
void QueueStack :: push(int x){
    q2.push(x);
    while(!q1.empty()){
        q2.push(q1.front());
        q1.pop();
    }
    q1.swap(q2);
}

//Function to pop an element from stack using two queues. 
int QueueStack :: pop() {
    if(q1.empty()) return -1;
    int popped = q1.front();
    q1.pop();
    return popped;
}
